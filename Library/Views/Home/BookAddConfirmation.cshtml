@{
  Layout = "_Layout";
}

<h1>You added a book!</h1>

<div class="panel panel-info">
  <div class="panel-heading">
    <h3 class="panel-title">@Model.GetTitle()</h3>
  </div>
  <div class="panel-body">
    <p>Genre: @Model.GetGenre()</p>
    <p>Publish Date: @Model.GetPublishDate()</p>
  </div>
</div>
